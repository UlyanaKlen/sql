/*Запрос «Готового компьютера» */
CREATE TABLE Готового компьютера /*Это 2 таблица, сделанная мной*/ 
(
[Код компьютеров] COUNTER CONSTRAINT PrimaryKey 
PRIMARY KEY, /*Код заказа*/ 
[наименование сборных компьютеров] DATE NOT NULL, /**/  
);
/*Таблица «Заказ» */
CREATE TABLE Заказ /*Это 1 таблица, сделанная мной*/
(
[Код Заказа] COUNTER CONSTRAINT PrimaryKey 
PRIMARY KEY, /*Кодовое слово*/ 
[Компьютер №п/п] TEXT NOT NULL, /*Ввод текста*/ 
[Стоимость] TEXT NOT NULL, /*Ввод текста*/ 
[Итоговая стоимость ПК со сборкой] TEXT NOT NULL, /*Ввод текста*/ 
);
